{
  "name": "forms-generator",
  "version": "1.0.4",
  "description": "A library that helps with HTML forms",
  "author": {
    "name": "Andrey Sharapov",
    "email": "an.sh.1@yandex.ru"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/an-sh/forms-generator.git"
  },
  "private": false,
  "main": "lib/forms-generator.js",
  "dependencies": {
    "multiparty": "^4.1.0",
    "async": "^2.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "nyc vows --spec tests/*.js",
    "pipe-coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js --verbose"
  },
  "keywords": [
    "form",
    "forms",
    "i18n",
    "validation",
    "jade"
  ],
  "devDependencies": {
    "coveralls": "^2.11.4",
    "dom-compare": "~0.2.0",
    "i18n": "~0.8.0",
    "jade": "^1.8.0",
    "jsdom": "^3.0.0",
    "nyc": "^7.1.0",
    "vows": ">0.8.0"
  },
  "license": "MIT",
  "nyc": {
    "include": [
      "lib/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  }
}
